{
    "name": "tontonyoyo/api-object",
    "description": "Working with api data like PhP Object",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["api", "entity"],
    "authors": [
        {
            "name": "TontonYoyo",
            "homepage": "https://github.com/tontonyoyo"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "doctrine/annotations": "^1.6",
        "guzzlehttp/psr7": "^1.5@dev",
        "php-http/curl-client": "^2.0@dev",
        "php-http/httplug-bundle": "^1.0@dev",
        "zendframework/zend-diactoros": "^2.1@dev"
    },
    "autoload": {
        "psr-4": {
            "TontonYoyo\\ApiObjectBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
